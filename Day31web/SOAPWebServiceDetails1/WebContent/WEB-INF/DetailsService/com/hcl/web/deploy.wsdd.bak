<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from DetailsService WSDL service -->

  <service name="Details" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://web.hcl.com"/>
      <parameter name="wsdlServiceElement" value="DetailsService"/>
      <parameter name="schemaQualified" value="http://web.hcl.com"/>
      <parameter name="wsdlServicePort" value="Details"/>
      <parameter name="className" value="com.hcl.web.DetailsSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Details"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getName" qname="operNS:getName" xmlns:operNS="http://web.hcl.com" returnQName="retNS:getNameReturn" xmlns:retNS="http://web.hcl.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="getName"/>

  </service>
</deployment>
