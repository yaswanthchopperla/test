package com.hcl.Exp;
class C1{
	int a,b;
	public C1(){
		a=5;
		b=6;	
	}
	@Override
	public String toString() {
		return "C1 [a=" + a + ", b=" + b + "]";
	}
}
class C2 extends C1{
	public C2(int a,int b){
	}
}
public class Quiz2 {
	public static void main(String[] args) {
		C2 ob=new C2(3,56);
		System.out.println(ob);
	}

}
