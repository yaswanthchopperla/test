package com.hcl.Exp;
class Answer{
	void max(int a, int b){
		if(a>b){
			System.out.println(a);
		}else{
			System.out.println(b);
		}
	}
	void power(int a, int b){
		for (int i=1;i<b;i++) {
			a=a*i;
			}
		System.out.println(a);
		
	}
}
public class Task {
	public static void main(String[] args) {
	int a,b;
	Answer ob=new Answer();
	ob.max(1,4);
	ob.power(4,2);
	}
	

}
