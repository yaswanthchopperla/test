package com.hcl.java;

public class Perfect {
	public void perfect(int n){
		int sum=0;
		for(int i=1;i<n;i++){
			if(n%i==0){
				System.out.println(+i);
				sum=sum+i;
			}
			System.out.println(+sum);			
		}
	}

	public static void main(String[] args) {
		int n=6;
		Perfect obj=new Perfect();
		obj.perfect(n);

	}

}
