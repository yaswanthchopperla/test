package com.hcl.inh;

public class day {
	public void calculate(){
		String str="good";
		String str1="bad";
		 int i = 0;
	      for(char c: str.toCharArray()) {
	         i++;
	      }
	      System.out.println("Length of the given string ::"+i);
	      if(str==str1){
	    	  System.out.println("true");
	      }else{
	    	  System.out.println("false");
	      }
	   }	
	public static void main(String[] args) {
		day obj=new day();
		obj.calculate();
	}
	}






